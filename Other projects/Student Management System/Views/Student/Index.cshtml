@model List<Student_Management_System.Models.student>

<h2>Student List</h2>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.name</td>
                <td>@student.email</td>
                <td>@student.phone</td>
                <td>@student.address</td>
            </tr>
        }
    </tbody>
</table>
