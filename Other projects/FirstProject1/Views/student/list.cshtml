
@{
    ViewBag.Title = "list";
}

<h2>Students List</h2>

<table border="1" cellpadding="5" cellspacing="0">
    <thead>
        <tr>
            <th>StudentId</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Address</th>
            <th>UserId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.studentid</td>
                <td>@student.name</td>
                <td>@student.email</td>
                <td>@student.phone</td>
                <td>@student.address</td>
                <td>@student.uerid</td>
            </tr>
        }
    </tbody>
</table>

